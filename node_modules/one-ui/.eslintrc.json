{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "rules": {
    "no-prototype-builtins": "error",
    "no-template-curly-in-string": "warn",
    "no-unsafe-negation": "error",
    "array-callback-return": "error",
    "block-scoped-var": "error",
    "class-methods-use-this": "warn",
    "consistent-return": "error",
    "curly": "warn",
    "default-case": "error",
    "dot-location": [
      "warn",
      "property"
    ],
    "eqeqeq": "warn",
    "no-alert": "warn",
    "no-caller": "warn",
    "no-div-regex": "error",
    "no-eq-null": "warn",
    "no-extend-native": "error",
    "no-extra-bind": "warn",
    "no-extra-label": "warn",
    "no-floating-decimal": "error",
    "no-global-assign": "warn",
    "no-implicit-coercion": "warn",
    "no-invalid-this": "warn",
    "no-iterator": "warn",
    "no-param-reassign": "error",
    "no-useless-return": "warn",
    "no-with": "warn",
    "yoda": "warn",
    "strict": "warn",
    "no-undef-init": "warn",
    "no-unused-vars": "off",
    "no-use-before-define": "error",
    "array-bracket-spacing": [
      "warn",
      "always"
    ],
    "key-spacing": [
      "warn",
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "block-spacing": [
      "warn",
      "always"
    ],
    "brace-style": [
      "warn",
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "comma-dangle": [
      "warn",
      {
        "arrays": "never",
        "objects": "never",
        "imports": "never",
        "exports": "never",
        "functions": "never"
      }
    ],
    "comma-spacing": [
      "warn",
      {
        "before": false,
        "after": true
      }
    ],
    "computed-property-spacing": [
      "warn",
      "never"
    ],
    "func-call-spacing": [
      "warn",
      "never"
    ],
    "indent": [
      "warn",
      2
    ],
    "space-infix-ops": "warn",
    "no-duplicate-imports": "warn",
    "no-plusplus": "warn",
    "no-continue": "warn",
    "no-tabs": "warn",
    "prefer-const": "warn",
    "arrow-spacing": "warn",
    "prefer-template": "warn"
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jest": true,
    "jasmine": true
  },
  "globals": {
    "ONEAPM": true,
    "__": true
  }
}
